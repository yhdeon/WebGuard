{
    "manifest_version": 3,
    "name": "Network & Session Check",
    "version": "1.0",
    "permissions": [
      "webRequest",
      "webRequestBlocking",
      "cookies",
      "storage",
      "activeTab"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": [
      "<all_urls>"
    ],
    "action": {
      "default_popup": "popup.html"
    }
  }
  