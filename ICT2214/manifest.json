{
    "manifest_version": 3,
    "name": "Website Interceptor",
    "version": "1.0",
    "description": "Intercepts navigation and shows a confirmation dialog before proceeding.",
    "permissions": ["tabs", "scripting", "webNavigation"],
    "host_permissions": ["<all_urls>"],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ]
}
